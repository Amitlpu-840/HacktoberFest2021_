#includes<bits/stdc++.h>
#using namespace std;

int sum(int arr[],int n)              //array in function
{
int i,sum1=0;

for(i=0;i<n;i++)
{
  sum1 += arr[i];
}

return sum1;

}


int main(){
int n;
cout<<"enter the size of array"<<endl;
cin>>n;
int arr[n];

for(int i=0;i<n;i++){
cin>>arr[i]
}

int result;

result=sum(arr,n);              //function calling array passing
cout<<"sum="<<result<<endl;       //result printing
return 0;
}
